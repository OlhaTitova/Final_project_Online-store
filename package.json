{
    "name": "project-online-store",
    "version": "0.1.0",
    "private": true,
    "proxy": "https://intense-reaches-12245.herokuapp.com/api",
    "dependencies": {
        "@testing-library/jest-dom": "^5.11.9",
        "@testing-library/react": "^11.2.5",
        "@testing-library/user-event": "^12.7.1",
        "antd": "^4.12.3",
        "axios": "^0.21.1",
        "node-sass": "^4.14.1",
        "prop-types": "^15.7.2",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-redux": "^7.2.2",
        "react-router-dom": "^5.2.0",
        "react-scripts": "4.0.2",
        "redux": "^4.0.5",
        "redux-devtools-extension": "^2.13.8",
        "redux-thunk": "^2.3.0",
        "styled-components": "^5.2.1",
        "web-vitals": "^1.1.0"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "parser": "babel-eslint",
        "env": {
            "browser": true,
            "mocha": true
        },
        "extends": [
            "standard",
            "react-app",
            "react-app/jest",
            "airbnb",
            "airbnb/hooks"
        ],
        "rules": {
            "no-trailing-spaces": [
                "error",
                {
                    "skipBlankLines": true
                }
            ],
            "no-return-assign": 0,
            "eol-last": 0,
            "semi": 0,
            "camelcase": 0,
            "object-curly-spacing": 0,
            "comma-dangle": 0,
            "linebreak-style": 0
        },
        "settings": {
            "react": {
                "createClass": "createReactClass",
                "pragma": "React",
                "version": "16.7.0",
                "flowVersion": "0.86"
            },
            "propWrapperFunctions": [
                "forbidExtraProps"
            ]
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "eslint": "^7.20.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-react": "^7.22.0",
        "eslint-plugin-react-hooks": "^1.7.0",
        "standard": "^16.0.3"
    }
}
